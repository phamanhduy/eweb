{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      },
      {
          "src": "public/**",
          "use": "@vercel/static"
      },
      {
        "src": "videos/**",
        "use": "@vercel/static"
      }
  ],
  "routes":[
      {
          "src": "/(.*)",
          "dest": "/"
      },
      {
          "src": "/",
          "dest": "public/index.html"
      },
      {
          "src": "/(.+)",
          "dest": "public/$1"
      }
  ]
}
